<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PreencherPrograma</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
       ISPICKVAL( Programa__c ,&apos;Desenvolvimento&apos;),
       ISPICKVAL ( SexoTurma__c, &apos;&apos;)

)</errorConditionFormula>
    <errorDisplayField>SexoTurma__c</errorDisplayField>
    <errorMessage>Preencher o campo Sexo da turma.</errorMessage>
</ValidationRule>
